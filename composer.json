{
  "name": "jcaillot/owasp-headers",
  "type": "library",
  "description": "Laravel middleware. Adds OWASP recommended headers to the response,",
  "keywords": [
    "framework",
    "laravel",
    "middleware",
    "OWASP",
    "http",
    "headers"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "jerome caillot",
      "email": "jerome@chaman.ca"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "laravel/framework": "^5.2 || ^6.0 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.98",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "post-update-cmd": [
      "@php -r \"file_exists('./config/owasp-headers2.php') || copy( 'config/owasp-headers-example.php', 'config/owasp-headers2.php');\""
    ]
  },
  "autoload": {
    "psr-4": {
      "Chaman\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Chaman\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}