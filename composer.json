{
    "name": "jcaillot/owasp-headers",
    "type": "library",
    "description": "Laravel middleware. Adds OWASP recommended headers to the response,",
    "keywords": [
        "framework",
        "laravel",
        "middleware",
        "OWASP",
        "http",
        "headers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "jerome caillot",
            "email": "jerome@chaman.ca"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^5.2 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpstan/phpstan": "^0.12.98",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "sniff": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
        ],
        "lint": [
            "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
        ]
    },
    "autoload": {
        "psr-4": {
            "Chaman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaman\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
